{
    "providers": {
        "$LLM_PROVIDER": {
            "api_key": "$API_KEY"
        }
    },
    "agents": {
        "defaults": {
            "model": "$LLM_MODEL"
        }
    },
    "channels": {
        "telegram": {
            "enabled": true,
            "token": "$TELEGRAM_BOT_TOKEN",
            "allowFrom": [
                "$TELEGRAM_ID"
            ]
        }
    }
}
